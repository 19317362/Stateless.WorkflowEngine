        Stateless.WorkflowEngine Console
      </a>
      <ul class="nav navbar-nav pull-right">
        <li class="dropdown">
          <a class="dropdown-toggle" data-toggle="dropdown" href="http://lab2023.github.io/hierapolis/dashboard.html#">
            <i class="icon-envelope"></i>
            Messages
            <span class="badge">5</span>
            <b class="caret"></b>
          </a>
          <ul class="dropdown-menu">
            <li>
              <a href="http://lab2023.github.io/hierapolis/dashboard.html#">New message</a>
            </li>
            <li>
              <a href="http://lab2023.github.io/hierapolis/dashboard.html#">Inbox</a>
            </li>
            <li>
              <a href="http://lab2023.github.io/hierapolis/dashboard.html#">Out box</a>
            </li>
            <li>
              <a href="http://lab2023.github.io/hierapolis/dashboard.html#">Trash</a>
            </li>
          </ul>
        </li>
        <li>
          <a href="http://lab2023.github.io/hierapolis/dashboard.html#">
            <i class="icon-cog"></i>
            Settings
          </a>
        </li>
        <li class="dropdown user">
          <a class="dropdown-toggle" data-toggle="dropdown" href="http://lab2023.github.io/hierapolis/dashboard.html#">
            <i class="icon-user"></i>
            <strong>John DOE</strong>
            <b class="caret"></b>
          </a>
          <ul class="dropdown-menu">
            <li>
              <a href="http://lab2023.github.io/hierapolis/dashboard.html#">Edit Profile</a>
            </li>
            <li class="divider"></li>
            <li>
              <a href="@Actions.Login.Logout">Sign out</a>
            </li>
          </ul>
        </li>
      </ul>
    </div>
    <div id="wrapper">
      <!-- Sidebar -->
        <section id="sidebar">
            <i class="icon-align-justify icon-large" id="toggle"></i>
            <ul id="dock">
                <li class="active launcher">
                <i class="fa fa-dashboard"></i>
                <a href="@Actions.Dashboard.Default">Dashboard</a>
                </li>
                <li class="launcher">
                <i class="fa fa-users"></i>
                <a href="@Actions.User.Default">Users</a>
                </li>
            </ul>
            <div data-toggle="tooltip" id="beaker" title="" data-original-title="Made by lab2023"></div>
        </section>
        @RenderBody()
    </div>
    <!-- Footer -->
    <!-- Javascripts -->
    <script src="/Content/Js/jquery.min.js"></script>
    <script src="/Content/Js/jquery-ui.min.js" type="text/javascript"></script>
    <script src="/Content/Js/bootstrap.min.js"></script>
    <script src="/Content/Js/hierapolis.js" type="text/javascript"></script>
    <script src="/Content/Js/metisMenu.min.js"></script>
    <script src="/Content/Js/modernizr.min.js" type="text/javascript"></script>
    <script src="/Content/Js/bootstrap-multiselect.js"></script>
